<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

  <Button
      android:id="@+id/intentuno"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="1er explicito"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      android:background="@color/black"
      android:onClick="onClick"
      android:layout_marginTop="10dp"
      />

  <Button
      android:id="@+id/intentdos"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="2do explicito"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/intentuno"
      android:background="@color/black"
      android:onClick="onClick"
      android:layout_marginTop="10dp"
      />

  <Button
      android:id="@+id/intentres"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="3er explicito"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/intentdos"
      android:background="@color/black"
      android:onClick="onClick"
      android:layout_marginTop="10dp"
      />

  <Button
      android:id="@+id/intentcuatro"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="4to explicito"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/intentres"
      android:background="@color/black"
      android:onClick="onClick"
      android:layout_marginTop="10dp"
      />

  <Button
      android:id="@+id/intentcinco"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="FACEBOOK"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      android:background="@color/black"
      android:onClick="facebook"
      android:layout_marginBottom="15dp"
      />

  <Button
      android:id="@+id/intentseis"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="GOOGLE"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintBottom_toTopOf="@+id/intentcinco"
      android:background="@color/black"
      android:onClick="google"
      android:layout_marginBottom="15dp"
      />

  <Button
      android:id="@+id/intentsiete"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="YOUTUBE"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintBottom_toTopOf="@+id/intentseis"
      android:background="@color/black"
      android:onClick="youtube"
      android:layout_marginBottom="15dp"
      />

  <Button
      android:id="@+id/intentocho"
      android:layout_width="350dp"
      android:layout_height="70dp"
      android:text="WHATSAPP"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintBottom_toTopOf="@+id/intentsiete"
      android:background="@color/black"
      android:onClick="whats"
      android:layout_marginBottom="15dp"
      />


</androidx.constraintlayout.widget.ConstraintLayout>